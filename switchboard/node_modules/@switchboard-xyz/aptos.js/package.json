{
  "name": "@switchboard-xyz/aptos.js",
  "version": "3.0.0",
  "description": "API wrapper for intergating with the Switchboardv2 program on Aptos",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/switchboard-xyz/sbv2-aptos.git",
    "directory": "packages/aptos.js"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./generated": {
      "types": "./generated.d.ts",
      "import": "./generated.js",
      "require": "./generated.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "lib/",
    "package.json",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "generated.cjs",
    "generated.js",
    "generated.d.ts"
  ],
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.13.3",
    "aptos": "^1.11.0",
    "chalk": "^5.0.1",
    "js-sha3": "^0.8.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.1.6",
    "@switchboard-xyz/common": "^2.3.22"
  },
  "devDependencies": {
    "@switchboard-xyz/eslint-config": "latest",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.11",
    "esbuild": "^0.17.19",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "yaml": "^2.1.1",
    "yargs": "^17.5.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "node esbuild.js",
    "build:old": "shx rm -rf lib || true; pnpm exec tsc -p tsconfig.cjs.json && pnpm exec tsc",
    "clean": "pnpm exec rimraf node_modules lib .turbo",
    "fix": "pnpm exec eslint --fix src/**/*.ts ./test",
    "lint": "pnpm exec eslint src/**/*.ts ./test",
    "test": "node ./node_modules/mocha/bin/mocha --exit",
    "watch": "pnpm exec tsc -p tsconfig.cjs.json --watch"
  }
}